{
  "name": "CCL",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clear_watchman": "watchman watch-del-all",
    "clear_react": "rm -rf $TMPDIR/react-*",
    "clear_node_modules": "rm -rf node_modules",
    "clear_npm": "npm cache clean --force",
    "clear_package": "rm -rf package-lock.json && rm -rf yarn.lock",
    "clear_ios_build": "rm -rf ios/build",
    "clear_pods": "rm -rf ios/Podfile.lock && rm -rf ios/Pods",
    "clear_pod_cache": "cd ios && pod cache clean --all && cd ..",
    "clear_ios_pod_clean": "yarn clear_ios_build && yarn clear_pods && yarn clear_pod_cache",
    "clear_ios": "yarn clear_ios_build && yarn clear_pods",
    "clear_android_app_build": "rm -rf android/app/build",
    "clear_android_build": "rm -rf android/build",
    "clear_android_gradle": "rm -rf android/.gradle",
    "clear_android_idea": "rm -rf android/.idea",
    "clear_android_clean": "cd android && ./gradlew clean && cd ..",
    "clear_adnroid": "yarn clear_android_app_build && yarn clear_android_build && yarn clear_android_gradle && yarn clear_android_idea",
    "android_build": "cd android && ./gradlew assembleDebug && cd ..",
    "clear_pod_clean": "yarn clear_watchman && yarn clear_react && yarn clear_node_modules && yarn clear_npm && yarn clear_package && yarn clear_ios_pod_clean && yarn clear_adnroid",
    "clear": "yarn clear_watchman && yarn clear_react && yarn clear_node_modules && yarn clear_npm && yarn clear_package && yarn clear_ios && yarn clear_adnroid",
    "pods": "npx pod-install ios",
    "fresh": "yarn clear && yarn && yarn pods && npx jetifier",
    "open-android-dev-menu": "adb shell input keyevent 82",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && open ./app/build/outputs/apk/release/",
    "bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && open ./app/build/outputs/bundle/release/",
    "release:android": "npx react-native run-android --mode=release",
    "release:ios": "npx react-native run-ios --mode Release"
  },
  "dependencies": {
    "@react-navigation/native": "6.x",
    "@react-navigation/native-stack": "6.x",
    "react": "18.2.0",
    "react-native": "0.74.7",
    "react-native-date-picker": "5.0.0",
    "react-native-safe-area-context": "4.14.1",
    "react-native-screens": "3.35.0",
    "react-native-svg": "15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.89",
    "@react-native/eslint-config": "0.74.89",
    "@react-native/metro-config": "0.74.89",
    "@react-native/typescript-config": "0.74.89",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}